public class Main
{


    static int hmd(String a, String b)//Hamming Distance Function
    {
        int i = 0;
        int count = 0;
        while (i < a.length())
        {
            if (a.charAt(i) != b.charAt(i))
                count++;
            i++;
        }
        return count;
    }
 
    public static void main(String[] args) {
        String t="@Azeeza2";
        String s="@Azeezat";
        
      
        
        System.out.printf("Azeezat Osikoya,oluwatobiosikoya1@gmail.com,@azeeza2, Drug Development,@Azeezat,%d",hmd(s,t)); 
    }
}
